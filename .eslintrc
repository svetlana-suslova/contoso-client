{
  "root": true,
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "react-app",
  "globals": {
    "baseUrl": true
  },
  "rules": {
    "no-var": "warn",
    "react-hooks/exhaustive-deps": "off",
    "no-throw-literal": "off",
    "no-console": "warn",
    "no-use-before-define": "off",
    "@typescript-eslint/no-use-before-define": "off",
    "import/no-anonymous-default-export": ["error", {
      "allowObject": true
    }]
  }
}
